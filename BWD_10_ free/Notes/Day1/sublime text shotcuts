	Ctrl + S -save
	Ctrl Shit + N -new folder
	Tab -autoformfill
	Ctrl + C -copy
	Ctrl + V -paste
	Ctrl +  X -cut
	Ctrl + D -multiple selection
	Ctrl + Enter -miidle to next line
	Ctrl + Z -back
